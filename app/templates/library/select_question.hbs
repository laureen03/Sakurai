<div class="container select-question">
	<h3 class="margin-top-55">{{t "common.questionTypes.questionType"}}</h3>
    <h4>{{t "common.questionTypes.questionTypeSelect"}}</h4>

    <ul class="question-type-list">
    {{#each questionTypes as |questionType|}}
        <li>
            <a href="#"{{action "createQuestion" questionType.type }}>{{questionType.label}}</a>
            {{questionType.desc}}
        </li>
    {{/each}}
    </ul>
</div>

