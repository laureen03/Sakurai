<div class="main clearfix" id="main" role="main">
    <section class="main-layout clearfix content-wrapper quiz-content">

        <div class="container cf animate-quiz-content">
            <h1 class="section-header dark">
                {{#if model.isExam}}
                    {{t "exam.resultAnalyses"}}
                {{else}}
                    {{t "quiz.analyzingResults"}}
                {{/if}}
            </h1>
            <div class="div1 align-center">
                <div class="circle incomplete">1</div>
                <h3 class="gray-text">
                    {{#if model.isExam}}
                        {{t "exam.grading"}}
                    {{else}}
                        {{t "quiz.gradingQuiz"}}
                    {{/if}}
                </h3>
            </div>
            <div class="div2 align-center">
                <div class="circle incomplete">2</div>
                <h3>
                    {{#if model.isExam}}
                        {{t "exam.analysingResults"}}
                    {{else}}
                        {{t "quiz.comparingPerformance"}}
                    {{/if}}
                </h3>
            </div>
            <div class="div3 align-center">
                <div class="circle incomplete">3</div>
                <h3 class="gray-text">
                    {{#if model.isExam}}
                        {{t "exam.preparingSummary"}}
                    {{else}}
                        {{#if model.selectedQuiz.isQuestionCollectionQuiz}}
                            {{t "quiz.calculatingScore"}}
                        {{else}}
                            {{t "quiz.calculatingMasteryLevel"}}
                        {{/if}}
                    {{/if}}
                </h3>
            </div>
        </div>

    </section>
</div>

